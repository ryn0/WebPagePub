@model WebPagePub.Web.Models.RedirectPathCreateModel
@{
    ViewData["Title"] = "Create Redirect Path";
}
<h2>@ViewData["Title"]</h2>

<form asp-controller="RedirectPathManagement" asp-action="Create" method="post" class="form-horizontal">

    <div class="form-group">
        <label asp-for="Path" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="Path" class="form-control" typ="text" />
        </div>
    </div>
    <div class="form-group">
        <label asp-for="PathDestination" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="PathDestination" class="form-control" typ="text" />
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="submit" class="btn btn-default">Create</button>
        </div>
    </div>
</form>


