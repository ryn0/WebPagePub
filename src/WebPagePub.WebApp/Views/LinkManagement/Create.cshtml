@model WebPagePub.Web.Models.LinkEditModel
@{
    ViewData["Title"] = "Create Link";
}

<h2>@ViewData["Title"]</h2>

 

<form asp-controller="LinkManagement" asp-action="Create" method="post" class="form-horizontal">
 
    <div class="form-group">
        <label asp-for="LinkKey" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="LinkKey" class="form-control" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="UrlDestination" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="UrlDestination" class="form-control" />
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="submit" class="btn btn-default">Create</button>
        </div>
    </div>

</form>


